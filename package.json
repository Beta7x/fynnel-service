{
  "name": "backend",
  "version": "1.0.0",
  "description": "Web service for Fynnel app",
  "keywords": [
    "Tunnel"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/Beta7x/Fynnel-backend"
  },
  "license": "MIT",
