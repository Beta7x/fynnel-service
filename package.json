    "typescript": "^5.9.2",
    "ws": "^8.18.3"
  },
  "dependencies": {
    "@newrelic/winston-enricher": "^4.0.1",
    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "newrelic": "^13.3.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
